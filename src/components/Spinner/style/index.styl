@import '../../style/';

.hiui-spinner {
  display: inline-block;
  height: 26px;
  width: 26px;
  border: 8px solid #666;
  border-right-color: transparent!important;
  border-radius: 50%;
  animation: rotate .75s linear infinite;
}

@keyframes rotate {
  form {
    transform: rotate(0deg);
  }
  to {
    transform: rotate(360deg);
  }
}
